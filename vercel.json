{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "public/*", "use": "@vercel/static" },
    { "src": "views/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/login", "dest": "/views/login.html" },
    { "src": "/register", "dest": "/views/register.html" },
    { "src": "/carrito", "dest": "/views/carrito.html" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}


  